{
  "name": "@react-docgen/cli",
  "version": "1.0.0-alpha.1",
  "description": "A CLI to extract information from React components for documentation generation.",
  "repository": "reactjs/react-docgen",
  "type": "module",
  "bin": {
    "react-docgen": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.17.0"
  },
  "scripts": {
    "build": "rimraf dist/ && tsc",
    "watch": "rimraf dist/ && tsc --watch"
  },
  "keywords": [
    "react",
    "cli",
    "documentation-generation"
  ],
  "author": {
    "name": "Daniel Tschinder (http://github.com/danez)"
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "5.2.0",
    "commander": "9.4.1",
    "debug": "4.3.4",
    "fast-glob": "3.2.12",
    "react-docgen": "workspace:6.0.0-alpha.4",
    "slash": "5.0.0"
  },
  "devDependencies": {
    "@types/debug": "4.1.7"
  }
}
