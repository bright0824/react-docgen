  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.18.9",
    "@babel/traverse": "^7.18.9",
    "@babel/types": "^7.18.9",
    "@types/babel__traverse": "^7.18.0",
    "@types/doctrine": "^0.0.5",
    "@types/resolve": "^1.20.2",
    "doctrine": "^3.0.0",
    "resolve": "^1.22.1",
    "strip-indent": "^3.0.0"
  }
}
