{
  "name": "react-docgen",
  "version": "6.0.0-alpha.3",
  "description": "A library to extract information from React components for documentation generation.",
  "repository": "reactjs/react-docgen",
  "browser": {
    "./src/importer/makeFsImporter.ts": "./src/importer/makeIgnoreImports.ts",
    "./dist/importer/makeFsImporter.js": "./dist/importer/makeIgnoreImports.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
