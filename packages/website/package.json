{
  "name": "@react-docgen-internal/website",
  "private": true,
  "version": "0.0.0",
  "description": "react-docgen website",
  "scripts": {
    "dev": "nx exec -- next dev",
    "build": "NODE_ENV=production nx exec -- next build",
    "start": "nx exec -- next start"
  },
  "author": "Daniel Tschinder (http://github.com/danez)",
  "license": "MIT",
  "dependencies": {
    "@codemirror/lang-javascript": "6.1.5",
    "@codemirror/lang-json": "6.0.1",
    "@codemirror/view": "6.9.3",
    "@headlessui/react": "1.7.13",
    "@popperjs/core": "2.11.7",
    "@types/react-dom": "18.0.11",
    "@uiw/react-codemirror": "4.19.11",
    "clsx": "1.2.1",
    "next": "13.3.0",
    "next-themes": "0.2.1",
    "nextra": "2.3.0",
    "nextra-theme-docs": "2.3.0",
    "postcss": "8.4.21",
    "postcss-lightningcss": "0.7.0",
    "react": "18.2.0",
    "react-docgen": "workspace:6.0.0-beta.6",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.1"
  },
  "devDependencies": {
    "@types/react": "18.0.33"
  },
  "browserslist": [
    "chrome 64",
    "edge 79",
    "firefox 67",
    "opera 51",
    "safari 12"
  ],
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          "{projectRoot}/.next"
        ]
      },
      "dev": {
        "dependsOn": [
          "^build"
        ]
      },
      "start": {
        "dependsOn": [
          "build"
        ]
      }
    }
  }
}
