{
  "compilerOptions": {
    "jsx": "react",
    "declaration": true,
    "lib": ["es2020"],
    "target": "es2020",
    "module": "commonjs",
    "strict": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "inlineSourceMap": false,
    "skipLibCheck": true
  }
}
